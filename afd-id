

estado_actual = 'A'
estados_finales = 'Z'

if estado_actual == 'A' and caracter.isalpha():
    estado_actual = 'Z'
elif estado_actual == 'A' and not caracter.isalpha():
    estado_actual = 'X'
elif estado_actual == 'Z' and caracter.isalnum():
    estado_actual = 'Z'
elif estado_actual == 'X' and caracter.isascii():
    estado_actual = 'X'


if  estado_actual == estados_finales:
    '''genera token'''